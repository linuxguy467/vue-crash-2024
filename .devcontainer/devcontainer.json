{
    "name": "Vue Crash 2024",
    "dockerComposeFile": "compose-dev.yaml",
    "service": "vue-app",
    "workspaceFolder": "/app",
    "customizations": {
        "vscode": {            
            "extensions": [
                "dbaeumer.vscode-eslint",
                "octref.vetur",
                "Vue.volar",
                "esbenp.prettier-vscode",
                "eamodio.gitlens"
            ],
            "settings": { 
                "terminal.integrated.shell.linux": "/bin/bash"
            }
        }
    },
    "forwardPorts": [3000]
}